{
    "manifest_version": 2,
    "name": "turtleTab, by Twenty Turtles",
    "short_name": "turtleTab",
    "description": "A Momentum clone, by Twenty Turtles - TurtleTab is a Chingu Voyage build to learn project.",
    "version": "0.1.7",
    "permissions" : [
        "management",
        "geolocation",
        "storage",
        "tabs"
    ],
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "background": {
      "persistent": false,
      "scripts": [ "js/background.js" ]
   },
    "browser_action": {
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        },
        "default_title": "turtleTab"
    }
 }
